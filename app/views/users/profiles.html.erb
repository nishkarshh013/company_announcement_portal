<div class="all-post">
  <div class="card" style="width: 18rem;">
    <div class="card-header">
      All Profiles
    </div>
    <ul class="list-group list-group-flush">
      <% @profiles.each do |profile|%>
        <li class="list-group-item"><%= link_to "#{profile.full_name}", user_path(profile)%></li>
      <% end %>
    </ul>
  </div>
</div>